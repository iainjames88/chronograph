    "bpk-component-panel": "^1.0.83",
    "bpk-component-text": "^1.0.81",
    "bpk-mixins": "^17.11.16",
    "bpk-stylesheets": "^3.2.128",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
